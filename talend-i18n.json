{
  "extract": {
    "method": "expression",
    "rootPath": "mvnModules",
    "script": "extract-i18n",
    "expression": "Messages.properties",
    "target": "./target/i18n"
  },
  "load": {
    "project": "Connectors-se",
    "fixQuote": true
  },
  "github": {
    "url": "https://github.com/Talend/connectors-se",
    "noCleanRepo": true,
    "localesBranchPrefix": "locales",
    "cloneFromBranch": true
  },
  "module": {
    "type": "mvn",
		"noDeploy": true
  }
}
